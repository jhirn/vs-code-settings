{
  // "apc.electron": {
  //   "opacity": 0.92
  // },
  "[dockerfile]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true,
    "editor.rulers": [
      120
    ],
    "editor.semanticHighlighting.enabled": true
  },
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSaveMode": "file",
    "editor.formatOnType": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSaveMode": "modifications",
    "editor.formatOnType": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "autoimport.showNotifications": true,
  "breadcrumbs.enabled": true,
  "browse-lite.startUrl": "http://localhost:3000",
  "cmake.options.statusBarVisibility": "compact",
  "css.validate": true,
  "cssVariables.lookupFiles": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "node_modules/@mantine/core/styles.css"
  ],
  "debug.console.fontSize": 14,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "never"
  },
  "editor.codeLens": true,
  "editor.cursorBlinking": "phase",
  "editor.detectIndentation": false,
  "editor.find.seedSearchStringFromSelection": "never",
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "Monaco, Menlo for Powerline, SF Mono, 'Courier New', monospace",
  "editor.fontWeight": "900",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": true,
  "editor.hover.delay": 250,
  "editor.indentSize": "tabSize",
  "editor.inlineSuggest.showToolbar": "onHover",
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.renderWhitespace": "trailing",
  "editor.scrollbar.horizontalScrollbarSize": 6,
  "editor.scrollbar.verticalScrollbarSize": 6,
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": false,
  "editor.stickyScroll.maxLineCount": 1,
  "editor.stickyScroll.scrollWithEditor": false,
  "editor.stickyTabStops": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  "emacs-mcx.emacsLikeTab": true,
  "emacs-mcx.strictEmacsMove": false,
  "emacs-mcx.useMetaPrefixMacCmd": true,
  "emmet.includeLanguages": {
    "erb": "html",
    "javascript": "javascriptreact",
    "postcss": "css",
    "typescript": "typescriptreact"
  },
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.debug": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.nodeEnv": "",
  "eslint.useESLintClass": true,
  "explorer.autoReveal": "focusNoScroll",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.experimental.affinity": {
    "pub.name": 1
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.css": "postcss",
    "*.strings": "plist"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.git/hooks": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "coverage/**": true,
    "tmp": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.liveshare.allowGuestAccess": false,
  "gitlens.showWelcomeOnInstall": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "hamlLint.executablePath": "bin/haml-lint",
  "indentRainbow.errorColor": "rgba(111,111,11,0.3)",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.strictFunctionTypes": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "postcss.validate": false,
  "python.defaultInterpreterPath": "python3",
  "ruby.specClearTerminal": false,
  "ruby.specCommand": "bin/rspec",
  "ruby.specSaveFile": true,
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "codeLens": false,
    "completion": true,
    "definition": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": true,
    "hover": true,
    "inlayHint": false,
    "onTypeFormatting": true,
    "references": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "signatureHelp": true,
    "workspaceSymbol": true
  },
  "rubyLsp.enableExperimentalFeatures": false,
  "rubyLsp.featuresConfiguration": {},
  "rubyLsp.formatter": "auto",
  "rubyLsp.indexing": {
    "excludedGems": [
      "rubocop",
      "rubocop-performance",
      "rubocop-rails"
    ],
    "excludedMagicComments": [
      "compiled:true"
    ],
    "excludedPatterns": [
      "**/test/**.rb",
      "**/spec/**/*.rb",
      "/app/frontend/**/*.*",
      "scratch.rb"
    ],
    "includedPatterns": [
      "**/bin/**"
    ]
  },
  "rubyLsp.pullDiagnosticsOn": "both",
  "rubyLsp.rubyVersionManager": {
    "identifier": "rbenv"
  },
  "search.exclude": {
    "**/*.code-search": true,
    "**/*.log": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "app/assets/builds/**": true,
    "dump*": true,
    "public/vite-dev/**": true,
    "public/vite/**": true,
    "rspec_status.txt": true,
    "storage/**": true,
    "tmp": true
  },
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.useIgnoreFiles": false,
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.keybindingsPerPlatform": false,
  "shellformat.useEditorConfig": true,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "plaintext": "javascript"
  },
  "tailwindCSS.lint.cssConflict": "error",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.osx": "fish-nix",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  // "terminal.integrated.env.osx": {
  //   "PATH": ""
  // },
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.profiles.osx": {
    "fish": {
      "args": [
        "-l"
      ],
      "path": "/opt/homebrew/bin/fish"
    },
    "fish-nix": {
      "args": [
        "-l"
      ],
      "path": "/run/current-system/sw/bin/fish",
      "env": {
        "PATH": "/run/current-system/sw/bin:$PATH"
      }
    }
  },
  "terminal.integrated.scrollback": 5000,
  "typescript.format.semicolons": "remove",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vite.autoStart": false,
  "vite.browserType": "system",
  "vite.port": 3036,
  "vs-browser.localProxyServer.enabled": true,
  "vscode-yaml-sort.emptyLinesUntilLevel": 1,
  "vsicons.dontShowNewVersionMessage": true,
  "window.nativeTabs": true,
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#88888844",
    "editor.lineHighlightBorder": "#00000000",
    "editorSuggestWidget.background": "#21252BCC",
    "editorSuggestWidget.foreground": "#FFFFFFAA",
    "editorWidget.background": "#21252BAA",
    "terminal.background": "#00000000"
  },
  "workbench.commandPalette.history": 100,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreviewFromCodeNavigation": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.editor.showTabs": "single",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.plist": "default"
  },
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "auto",
  "workbench.startupEditor": "none",
  "zenMode.hideLineNumbers": false,
  "doublebot.showInlineKeybindingHint": "Never",
  "window.doubleClickIconToClose": true,
  "circleci.hostUrl": "",
  "circleci.notifications.mute": true,
  "security.promptForLocalFileProtocolHandling": false,
  "cursor.cpp.disabledLanguages": [
    "plaintext",
    "markdown",
    "scminput"
  ],
  "terminal.integrated.allowChords": false,
  "window.customTitleBarVisibility": "never",
  "workbench.colorTheme": "Default Dark+",
  "diffEditor.ignoreTrimWhitespace": false,
  "sync.gist": "27a450d4f7f835d89a61cf88546f0a6d",
  "sync.autoUpload": true,
  "sync.forceUpload": true,
  "sync.quietSync": true,
  "workbench.settings.applyToAllProfiles": [
    "workbench.colorCustomizations"
  ],
  "window.titleBarStyle": "native",
}
